{
    "name": "web-chefs/puppeteertopdf",
    "description": "Convert a webpage to a pdf using headless Chrome",
    "homepage": "https://github.com/web-chefs/PuppeteerToPdf",
    "keywords":
    [
        "convert",
        "webpage",
        "pdf",
        "chrome",
        "headless",
        "puppeteer"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Fossey"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "spatie/temporary-directory": "^1.1",
        "symfony/process": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "spatie/phpunit-snapshot-assertions": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "WebChefs\\PuppeteerToPdf\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebChefs\\PuppeteerToPdf\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
